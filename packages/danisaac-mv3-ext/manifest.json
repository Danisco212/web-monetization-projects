{
    "manifest_version": 3,
    "name": "Daniel Isaac Injection Extension",
    "description": "This is a test injection extension made with mv3",
    "version": "1.0.0",
    "icons": {
        "16": "/images/icon16.png",
        "48": "/images/icon48.png",
        "128": "/images/icon128.png"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content-script.js"]
    }],
    "web_accessible_resources": [
        {
          "resources": [ "injection.js" ],
          "matches": [ "<all_urls>" ]
        }
      ]
}