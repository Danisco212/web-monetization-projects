{
  "$schema": "../coil-monorepo-upkeep/resources/package-json-schema-nested-overrides.json",
  "$overRideUpKeep": {
    "homepage": "",
    "scripts": {
      "test": "react-app-rewired test"
    }
  },
  "name": "@coil/extension-popup",
  "version": "0.0.0",
  "keywords": [
    "ilp",
    "web-monetization"
  ],
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "git@github.com:coilhq/web-monetization-projects.git"
  },
  "license": "Apache-2.0",
  "author": "Coil Team <info@coil.com>",
  "main": "./build",
  "types": "./build",
  "subpackages": [
    "assets",
    "assets/images",
    "components",
    "components/icons",
    "components/DesignIterations",
    "components/DesignIterations/list",
    "redux",
    "redux/actions",
    "redux/actions-constants",
    "redux/models",
    "redux/reducer",
    "redux/store",
    "theme",
    "types",
    "utils",
    "utils/get-amount-data.utils",
    "utils/get-page-data.util",
    "utils/redux-persist-storage.utils",
    "utils"
  ],
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "precommit": "echo lint-staged runs from root",
    "prettier": "prettier --write '*.{ts,tsx,js,html,jsx,md}' '{src,test}/**/*.{ts,tsx,js,html,jsx,md}'",
    "format": "yarn prettier && LINT_FIX=1 yarn lint:all --fix --quiet",
    "build:ts": "tsc --build tsconfig.build.json",
    "build:ts:watch": "yarn build:ts --watch",
    "build:ts:verbose": "yarn build:ts --verbose",
    "clean:build": "rimraf build",
    "upkeep": "cd ../.. && yarn upkeep",
    "lint:all": "yarn lint 'src/**/*.{ts,tsx}' 'test/**/*.{ts,tsx}'",
    "lint": "eslint --cache --cache-location ../../node_modules/.cache/eslint",
    "test:coverage": "yarn test --verbose --coverage"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@coil/client": "0.0.0",
    "@coil/extension-popup": "0.0.0",
    "@hookform/resolvers": "^1.3.5",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "customize-cra": "^1.0.0",
    "lodash.throttle": "^4.1.1",
    "react": "^17.0.1",
    "react-app-rewired": "^2.1.8",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.15.4",
    "react-redux": "^7.2.2",
    "react-scripts": "4.0.3",
    "redux": "^4.0.5",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-typescript": "^7.13.0",
    "@commitlint/cli": "^12.0.0",
    "@commitlint/config-conventional": "^12.0.0",
    "@types/jest": "^26.0.21",
    "@types/lodash.throttle": "^4.1.6",
    "@types/node": "^14.10.3",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/react-redux": "^7.1.16",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@yarnpkg/pnpify": "^2.4.0",
    "JSON2016": "^1.0.0",
    "copy-webpack-plugin": "^8.0.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-jsx-a11y": "^6.4.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-standard": "^5.0.0",
    "gh-pages": "^3.1.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lerna": "^4.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "redux-devtools-extension": "^2.13.9",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3",
    "webpack": "^5.27.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
